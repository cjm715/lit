journal = {SIAM Review},
keywords = {1,65m50,65n50,ams subject classifications,are nearly,be complex codes that,black boxes,delaunay triangulation,distance functions,inaccessible,introduction,mesh generation,mesh generators tend to,the meshing software is,they are often just,used as},
