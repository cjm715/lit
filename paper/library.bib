@article{Qian2002,
abstract = {Based on a recently developed formalism for mesoscopic stochastic dynamics of single macromolecules, such as motor proteins, in aqueous solution, we demonstrate mathematically the principle of the nonequilibrium thermodynamics originated by the Brussels group. The key concepts of excess entropy and excess entropy production, and their mathematical properties as well as physical interpretations, are discussed. The newly developed stochastic macromolecular mechanics is consistent with the general theory of nonequilibrium thermodynamics far from equilibrium, and more importantly, it bridges the abstract theory with the current experimental and modeling work on molecular motors and other biological systems in nonequilibrium steady state.},
author = {Qian, Hong},
