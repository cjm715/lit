journal = {IEEE Transactions on Automatic Control},
keywords = {Bilinear systems,Lie algebras,Schrodinger equation,controllability,infinite systems,infinite-dimensional control,lie theory,quantum control,quantum systems,trapped-ions},
mendeley-tags = {infinite systems,lie theory,quantum systems},
number = {8},
