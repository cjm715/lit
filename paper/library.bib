issn = {0022-1120},
journal = {Journal of Fluid Mechanics},
keywords = {micro-organism dynamics,particle-fluid flow,suspensions},
pages = {482--522},
title = {{Transport of a dilute active suspension in pressure-driven channel flow}},
url = {http://www.journals.cambridge.org/abstract{\_}S0022112015003729},
