isbn = {1947-5454},
issn = {1947-5454},
keywords = {collective effects,evolution,genes,statistical mechanics},
title = {{Life is physics: evolution as a collective phenomenon far from equilibrium}},
url = {http://arxiv.org/abs/1011.4125},
year = {2010}
