isbn = {0022112006008},
issn = {0022-1120},
journal = {Journal of Fluid Mechanics},
number = {-1},
pages = {289},
title = {{A bound on scalar variance for the advection–diffusion equation}},
url = {http://www.journals.cambridge.org/abstract{\_}S0022112006008639},
volume = {552},
year = {2006}
}
