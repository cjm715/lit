keywords = {Biomechanics,Computer Simulation,Hydrodynamics,Kinetics,Models,Movement,Suspensions,Theoretical},
number = {68},
pages = {571--85},
pmid = {21865254},
title = {{Emergence of coherent structures and large-scale flows in motile suspensions.}},
