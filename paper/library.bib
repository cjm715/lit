@article{JLT2012,
abstract = {Mixing is relevant to many areas of science and engineering, including the pharmaceutical and food industries, oceanography, atmospheric sciences and civil engineering. In all these situations one goal is to quantify and often then to improve the degree of homogenization of a substance being stirred, referred to as a passive scalar or tracer. A classical measure of mixing is the variance of the concentration of the scalar, which is the L 2 norm of a mean-zero concentration field. Recently, other norms have been used to quantify mixing, in particular the mix-norm as well as negative Sobolev norms. These norms have the advantage that unlike variance they decay even in the absence of diffusion, and their decay corresponds to the flow being mixing in the sense of ergodic theory. General Sobolev norms weigh scalar gradients differently, and are known as multiscale norms for mixing. We review the applications of such norms to mixing and transport, and show how they can be used to optimize the stirring and mixing of a decaying passive scalar. We then review recent work on the less-studied case of a continuously replenished scalar field—the source–sink problem. In that case the flows that optimally reduce the norms are associated with transport rather than mixing: they push sources onto sinks, and vice versa.},
author = {Thiffeault, Jean-Luc},
file = {:Users/cmiless/Documents/Mendeley Desktop//Thiffeault - 2012 - Using multiscale norms to quantify mixing and transport.pdf:pdf},
journal = {Nonlinearity},
keywords = {mixing},
mendeley-tags = {mixing},
number = {2},
pages = {R1},
title = {{Using multiscale norms to quantify mixing and transport}},
volume = {25},
year = {2012}
}
@article{Kruger2016,
abstract = {We report curling self-propulsion in aqueous emulsions of common mesogenic compounds. Nematic liquid crystal droplets self-propel in a surfactant solution with concentrations above the critical micelle concentration while undergoing micellar solubilization. We analyzed trajectories both in a Hele-Shaw geometry and in a 3D setup at variable buoyancy. The coupling between the nematic director field and the convective flow inside the droplet leads to a second symmetry breaking which gives rise to curling motion in 2D. This is demonstrated through a reversible transition to non-helical persistent swimming by heating to the isotropic phase. Furthermore, auto-chemotaxis can spontaneously break the inversion symmetry, leading to helical trajectories.},
archivePrefix = {arXiv},
arxivId = {1605.03396},
author = {Kr{\"{u}}ger, Carsten and Kl{\"{o}}s, Gunnar and Bahr, Christian and Maass, Corinna C.},
doi = {10.1103/PhysRevLett.117.048003},
eprint = {1605.03396},
file = {:Users/cmiless/Documents/Mendeley Desktop/1605.03396(1).pdf:pdf},
issn = {0031-9007},
journal = {Arxiv},
keywords = {Arxiv,liquid crystal},
pages = {1--5},
title = {{Curling Liquid Crystal Microswimmers: a cascade of spontaneous symmetry breaking}},
url = {http://arxiv.org/abs/1605.03396},
volume = {1},
year = {2016}
}
