issn = {1742-5689},
keywords = {astrobiology,biocomplexity,biophysics},
pmid = {23235265},
title = {{The Algorithmic Origins of Life}},
url = {http://arxiv.org/abs/1207.4803 http://dx.doi.org/10.1098/rsif.2012.0869},
year = {2012}
}
