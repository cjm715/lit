isbn = {0034-4885},
issn = {1361-6633},
journal = {Reports on progress in physics. Physical Society (Great Britain)},
keywords = {Feedback,Hot Temperature,Models,Molecular Motor Proteins,Molecular Motor Proteins: metabolism,Stochastic Processes,Theoretical,Thermodynamics},
number = {12},
pages = {126001},
