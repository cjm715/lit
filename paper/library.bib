@article{Otto1999a,
abstract = {We show that transport inequalities, similar to the one derived by Talagrand [30] for the Gaussian measure, are im-plied by logarithmic Sobolev inequalities. Conversely, Talagrand's inequality implies a logarithmic Sobolev inequality if the density of the measure is approximately log-concave, in a precise sense. All constants are independent of the dimension, and optimal in certain cases. The proofs are based on partial differential equations, and an interpolation inequality involving the Wasserstein distance, the entropy functional and the Fisher information.},
author = {Otto, F and Villani, Cedric},
doi = {10.1006/jfan.1999.3557},
file = {:Users/cmiless/Documents/Mendeley Desktop/Otto, Villani - 1999 - Generalization of an Inequality By Talagrand, and Links With the Logarithmic Sobolev Inequality(2).pdf:pdf},
