journal = {Ecology Letters},
keywords = {Abundance,Biodiversity,Macroecology,Power law,Scaling,Species-area relationship},
number = {8},
pages = {789--797},
pmid = {19486123},
title = {{Biodiversity scales from plots to biomes with a universal species-area curve}},
volume = {12},
