@article{Batchelor1952,
abstract = {A material line consisting always of the same particles of fluid is lengthened by the con- vective action of turbulent motion, at a rate which is shown to be exponential at a sufficient time after the initial choice of the line. The area of material surface elements also increases exponentially, although with a different exponential coefficient. It is then a consequence of conservation of mass of the fluid that the normal distance between two neighbouring parallel material surfaces decreases at an exponential rate which is simply related to the rate at which material lines lengthen. These kinematical results are relevant to the convective action of the turbulence on the distribution of two kinds of local property of the fluid, viz. quantities, represented by 0, of which the total amount in a material volume of the fluid remains constant (e.g. mass density of a foreign substance), and quantities, represented by F, of which the total flux across a material surface remains constant (e.g. vorticity). F is proportional, at all times, to the vector representing a material line element and G, = VO, is proportional to the vector representing a material surface element, and hence they both increase exponentially in time. The last two sections of the paper are concerned with the combined effect on F and G of convection and molecular diffusion, and a condition for the effect of the convection to be dominant is obtained. In the case of F, convection is found to be dominant if the appropriate molecular diffusivity is small compared with the kinematic viscosity of the fluid, and F2 will then increase indefinitely, unless some additional effect comes into play (as it does in the case of magnetic field strength). In the case of G, convection will be dominant if the initial distribution of G has a large enough length scale; the effect of the convection is to decrease this length scale so that ultimately the two effects reach an equilibrium. The length scale of the ultimate distribution of G, as obtained herein, is different from that put forward recently by Obukhoff.},
author = {Batchelor, G. K.},
doi = {10.1098/rspa.1952.0130},
file = {:Users/cmiless/Documents/Mendeley Desktop/Batchelor - 1952 - The effect of homogeneous turbulence on material lines and surfaces.pdf:pdf},
issn = {1364-5021},
journal = {Proceedings of the Royal Society A},
keywords = {mixing},
mendeley-tags = {mixing},
number = {1114},
pages = {349--366},
title = {{The effect of homogeneous turbulence on material lines and surfaces}},
volume = {213},
year = {1952}
}
@article{Okabe2008,
abstract = {Mixing refers to the homogenization of$\backslash$nconcentrations of passive scalars in fluids. On$\backslash$nsmall scales it is dominated by diffusion and on$\backslash$nlarge scales it is assisted by stirring. In the$\backslash$npresence of scalar sources and sinks the$\backslash$nconcentration field remains inhomogeneous, but the$\backslash$ncombined effect of stirring and dissipation may lead$\backslash$nto a statistically stationary state. One measure of$\backslash$nthe quality of mixing is then the standard deviation$\backslash$nof the scalar concentration from the mean, and the$\backslash$neffectiveness of a stirring velocity field can be$\backslash$ngauged by comparing the concentration fluctuations$\backslash$nin the presence of stirring to those in its$\backslash$nabsence. It was recently noted that the maximum$\backslash$npossible effectiveness of any stirring depends on$\backslash$nthe detailed structure of the sources and sinks. We$\backslash$npresent results from particle-based simulations that$\backslash$nconfirm this strong source-sink dependence of the$\backslash$nmixing enhancement by stirring.},
archivePrefix = {arXiv},
arxivId = {0804.2805},
author = {Okabe, T. and Eckhardt, B. and Thiffeault, J.-L. and Doering, C. R.},
doi = {10.1088/1742-5468/2008/07/P07018},
eprint = {0804.2805},
file = {:Users/cmiless/Documents/Mendeley Desktop/OETD2008JSMTh{\&}E.pdf:pdf},
issn = {1742-5468},
journal = {Journal of Statistical Mechanics: Theory and Experiment},
keywords = {0804,2805,arxiv eprint,brownian motion,heat,stochastic particle dynamics,theory,transfer,transport processes,turbulence},
number = {7},
pages = {P07018},
title = {{Mixing effectiveness depends on the source--sink structure: {\{}S{\}}imulation results}},
url = {http://stacks.iop.org/1742-5468/2008/P07018},
volume = {2008},
year = {2008}
}
@article{Bochkov1977,
abstract = {A method is proposed for investigating the thermal fluctuations in an arbitrary system subjected to the$\backslash$ninfluence of a dynamic perturbation (and in contact with a thermostat). An exact formula is obtained for$\backslash$nthe characteristic functional of the fluctuations of the macroscopic variables. The formula contains all the$\backslash$nmost general consequences of the basic postulates of statistical mechanics regarding the macroscopic$\backslash$nvariables. An infinite set of relations between the moment and cumulant functions of the equilibrium and$\backslash$nnonequilibrium fluctuations (in particular, the Onsager reciprocity relations and the fluctuation-dissipation$\backslash$ntheorems) follows from the formula. The consequences of the additional assumption that the macroscopic$\backslash$nvariables are Markovian are also considered.},
author = {Bochkov, G N and Kuzolev, Yu. E},
file = {:Users/cmiless/Documents/Mendeley Desktop/Bochkov, Kuzolev - 1977 - General theory of thermal fluctuations in nonlinear systems.pdf:pdf},
journal = {Sov. Phys. JETP},
pages = {125--130},
title = {{General theory of thermal fluctuations in nonlinear systems}},
volume = {45},
year = {1977}
}
