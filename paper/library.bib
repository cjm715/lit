isbn = {1090-7807},
issn = {10907807},
journal = {Journal of Magnetic Resonance},
keywords = {Cytochrome c,Double electron-electron resonance,Double-quantum coherence ESR,Maximum entropy,T4 lysozyme},
number = {2},
pages = {184--196},
pmid = {16137901},
title = {{Maximum entropy: A complement to Tikhonov regularization for determination of pair distance distributions by pulsed ESR}},
