month = {feb},
number = {5},
pages = {1697--702},
pmid = {24449879},
title = {{Acoustic droplet vaporization is initiated by superharmonic focusing.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24449879 http://www.pnas.org/lookup/doi/10.1073/pnas.1312171111},
volume = {111},
year = {2014}
}
