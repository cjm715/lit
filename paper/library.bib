author = {Zughbi, H. D. and Khokhar, Z. H. and Sharma, R. N.},
doi = {10.1021/ie0209935},
file = {:Users/cmiless/Documents/Mendeley Desktop/Zughbi, Khokhar, Sharma - 2003 - Mixing in pipelines with side and opposed tees.pdf:pdf},
