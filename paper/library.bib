issn = {1539-3755},
journal = {Physical Review E},
number = {1},
pages = {012317},
pmid = {26274171},
title = {{Suppression of Ostwald ripening in active emulsions}},
url = {http://link.aps.org/doi/10.1103/PhysRevE.92.012317},
volume = {92},
year = {2015}
}
